        "RECORD_AUDIO"
      ],
      "package": "com.franciscojoaopedro.iabela"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "6fd27b0d-ad7d-4396-8319-6fca292f55b1"
      }
    }
  }
